{{define "content"}}
<table class="table">
    {{if .Matches}}
    <tr>
        <th>When</th>
        <th>Map</th>
        <th>Duration</th>
        <th>Mode</th>
    </tr>
    {{range .Matches}}
    <tr>
        <td>
            <a href="matches/{{- .DataHash}}">
                {{.DateTime}}
            </a>
        </td>
        <td>{{.Map}}</td>
        <td>{{.Duration}}</td>
        <td>{{.Type}}</td>
    </tr>
    {{end}}
    {{else}}
    No matches to show
    {{end}}
</table>
{{end}}
